<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSearchView">
        <attr name="hintText" format="string"/>
        <attr name="backIcon" format="reference"/>
        <attr name="oneKeyCleanIcon" format="reference"/>
        <attr name="historyIcon" format="reference"/>
        <attr name="defaultState" format="enum">
            <enum name="close" value="0"/>
            <enum name="open" value="1"/>
        </attr>
        <attr name="historyTextColor" format="color"/>
        <attr name="inputView_height" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="AlphaTabView">
        <attr name="tabIconNormal" format="reference" />
        <attr name="tabIconSelected" format="reference" />
        <attr name="tabText" format="string" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="textColorNormal" format="color" />
        <attr name="textColorSelected" format="color" />
        <attr name="badgeBackgroundColor" format="color" />
        <attr name="paddingTexwithIcon" format="dimension"/>
    </declare-styleable>
</resources>
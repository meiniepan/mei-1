<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSearchView">
        <attr name="hintText" format="string" />
        <attr name="backIcon" format="reference" />
        <attr name="oneKeyCleanIcon" format="reference" />
        <attr name="historyIcon" format="reference" />
        <attr name="defaultState" format="enum">
            <enum name="close" value="0" />
            <enum name="open" value="1" />
        </attr>
        <attr name="historyTextColor" format="color" />
        <attr name="inputView_height" format="dimension" />

    </declare-styleable>

    <declare-styleable name="AlphaTabView">
        <attr name="tabIconNormal" format="reference" />
        <attr name="tabIconSelected" format="reference" />
        <attr name="tabText" format="string" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="textColorNormal" format="color" />
        <attr name="textColorSelected" format="color" />
        <attr name="badgeBackgroundColor" format="color" />
        <attr name="paddingTexwithIcon" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RadarView">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:text" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="labelTextColor" format="color" />
        <attr name="labelText" format="string" />
        <attr name="format" format="string" />
        <attr name="sideColor" format="color" />
        <attr name="outsideBackgroundColor" format="color" />
        <attr name="insideBackgroundColor" format="color" />
        <attr name="duration" format="integer" />
        <attr name="textOffsetY" format="dimension" />
        <attr name="labelTextOffsetY" format="dimension" />

        <attr name="circleColor" format="color" />
        <attr name="lineColor" format="color" />
        <attr name="showLine" format="boolean" />
        <attr name="rotate" format="integer" />
        <attr name="showLabel" format="boolean" />
        <attr name="showText" format="boolean" />
        <attr name="scanTime" format="integer" />
        <attr name="insideStrokeWidth" format="dimension" />
        <attr name="outsideStrokeWidth" format="dimension" />
        <attr name="lineStrokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PullRefreshLayout">
        <attr name="prl_refreshEnable" format="boolean" />
        <attr name="prl_loadMoreEnable" format="boolean" />
        <attr name="prl_twinkEnable" format="boolean" />
        <attr name="prl_autoLoadingEnable" format="boolean" />
        <attr name="prl_headerFront" format="boolean" />
        <attr name="prl_footerFront" format="boolean" />

        <attr name="prl_refreshTriggerDistance" format="dimension" />
        <attr name="prl_loadTriggerDistance" format="dimension" />
        <attr name="prl_pullDownMaxDistance" format="dimension" />
        <attr name="prl_pullUpMaxDistance" format="dimension" />
        <attr name="prl_topOverScrollMaxTriggerOffset" format="dimension" />
        <attr name="prl_downOverScrollMaxTriggerOffset" format="dimension" />

        <attr name="prl_resetAnimationDuring" format="integer" />
        <attr name="prl_refreshAnimationDuring" format="integer" />
        <attr name="prl_overScrollMinDuring" format="integer" />

        <attr name="prl_dragDampingRatio" format="float" />
        <attr name="prl_overScrollDampingRatio" format="float" />
        <attr name="prl_overScrollAdjustValue" format="float" />

        <attr name="prl_targetId" format="reference" />

        <attr name="prl_headerViewId" format="reference" />
        <attr name="prl_footerViewId" format="reference" />

        <attr name="prl_headerShowGravity" format="enum">
            <enum name="follow" value="0" />
            <enum name="follow_placeholder" value="1" />
            <enum name="follow_center" value="2" />
            <enum name="placeholder" value="3" />
            <enum name="placeholder_follow" value="4" />
            <enum name="placeholder_center" value="5" />
            <enum name="center" value="6" />
            <enum name="center_follow" value="7" />
        </attr>

        <attr name="prl_footerShowGravity" format="enum">
            <enum name="follow" value="0" />
            <enum name="follow_placeholder" value="1" />
            <enum name="follow_center" value="2" />
            <enum name="placeholder" value="3" />
            <enum name="placeholder_follow" value="4" />
            <enum name="placeholder_center" value="5" />
            <enum name="center" value="6" />
            <enum name="center_follow" value="7" />
        </attr>
    </declare-styleable>
</resources>